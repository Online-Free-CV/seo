{
  "name": "@onlinefreecv/seo",
  "version": "0.1.2",
  "description": "SEO toolkit for OnlineFreeCV â€“ shared utilities for titles, meta tags, canonicals, and JSON-LD across all templates.",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
